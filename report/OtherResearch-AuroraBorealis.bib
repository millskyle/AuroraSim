@article{Baranoski2003,
abstract = {We present an algorithm to simulate the aurora borealis, commonly$\backslash$nknown as the {\&}amp;ldquo;northern lights{\&}amp;rdquo;, a natural phenomenon of$\backslash$ngreat visual beauty and considerable scientific interest. The algorithm$\backslash$nis based on the current understanding of the physical origin of the$\backslash$naurora. This natural display is mainly caused by high-energy electrons$\backslash$noriginating in the Sun and entering the Earth's atmosphere in narrow$\backslash$nregions centered on the magnetic poles. These electrons collide with$\backslash$natmospheric atoms which are excited to higher energy levels. The excited$\backslash$natoms emit rapidly varying visible light in a curtain-like volume as$\backslash$nthey return to lower energy levels thereby creating the aurora. By$\backslash$nsimulating these light emissions along with the spatial and temporal$\backslash$ndistribution of the entering electrons, we are able to render the major$\backslash$nvisual aspects of auroral displays. This approach also allows the$\backslash$nrepresentation of time-dependent features that characterize the dynamic$\backslash$nnature of the aurorae. The applicability of this auroral model for$\backslash$nartistic and research purposes is illustrated through comparisons of$\backslash$nsynthetic images with photographs of real auroral displays},
author = {Baranoski, Gladimir V G and Rokne, Jon G. and Shirley, Peter and Trondsen, Trond S. and Bostos, Rui},
