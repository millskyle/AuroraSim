@article{Blixt2006,
abstract = {Optical observations of the aurora have traditionally focused on the$\backslash$nstructure, intensity, and wavelength of the emissions. But the apparent$\backslash$nmotion of auroral forms offers another important diagnostic tool$\backslash$nfor investigating this poorly understood phenomenon. Prior analyses$\backslash$nof auroral motion have focused on tracing individual features. In$\backslash$nthis letter, we investigate the feasibility of deriving the entire$\backslash$ntwo-dimensional velocity field automatically using robust optical$\backslash$nflow estimation. The analysis is applied to two narrow-field video$\backslash$nsequences. Both examples are rich in small-scale structure and motion,$\backslash$nbut appear very different to the eye. The robust optical flow estimator$\backslash$nperformed well for regions of dense turbulent motion, while sheared$\backslash$nflow, and flow which is perpendicular to image intensity gradients,$\backslash$nwas poorly resolved. The relative magnitude of the outliers provides$\backslash$na quantitative measure of the validity of the underlying flow model$\backslash$nand, hence, a means of automatically differentiating among auroral$\backslash$nforms with differing physical origins.},
author = {Blixt, Erik Marten and Semeter, Joshua and Ivchenko, Nickolay},
doi = {10.1109/LGRS.2005.860981},
file = {:home/kmills/Dropbox/Mendeley/2006/Blixt, Semeter, Ivchenko/Blixt, Semeter, Ivchenko{\_}2006{\_}IEEE Geoscience And Remote Sensing Letters{\_}Optical flow analysis of the aurora borealis.pdf:pdf},
issn = {1545-598X},
journal = {IEEE Geoscience And Remote Sensing Letters},
number = {1},
pages = {159--163},
title = {{Optical flow analysis of the aurora borealis}},
volume = {3},
year = {2006}
}
