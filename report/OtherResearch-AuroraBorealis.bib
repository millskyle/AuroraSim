issn = {02731177},
journal = {Advances in Space Research},
keywords = {Carrington event,Carrington event aurora,Eyewitness reports,Great aurora},
number = {2},
pages = {145--154},
title = {{Eyewitness reports of the great auroral storm of 1859}},
volume = {38},
year = {2006}
}
